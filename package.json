{
  "name": "license-info-webpack-plugin",
  "version": "1.1.0",
  "description": "Making a list of package's LICENSE information for webpack",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "watch": "babel -w src/index.js -o lib/index.js",
    "build": "NODE_ENV=production babel src/index.js -o lib/index.js",
    "prettier": "prettier --write '{src,__tests__}/**/*.js'",
    "fixtures:v3": "cd __tests__/v3/ && yarn install",
    "fixtures:v4": "cd __tests__/v4/ && yarn install",
    "lint": "eslint",
    "jest": "jest",
    "test": "npm-run-all fixtures:* jest",
    "version": "npm run build"
  },
  "author": "Takahiro Abe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yami-beta/license-info-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "license",
    "plugin"
  ],
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)",
      "**/__tests__/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "memory-fs": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "webpack-sources": "^1.3.0"
  }
}
