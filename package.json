    "babel-preset-env": "^1.1.8",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
